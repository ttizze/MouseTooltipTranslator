{
  "name": "mouse_tooltip_translator_dev",
  "version": "0.1.87",
  "description": "Mouse over to translate",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "zip": "npm-build-zip",
    "delete": "del-cli --force ./build",
    "build-zip": "npm run delete && npm run build && npm run zip",
    "delete-en": "del-cli --force ./build/_locales/*  !./build/_locales/en",
    "zip-en": "npm-build-zip --name _en",
    "build-zip-en": "npm run build-zip && npm run delete-en && npm run zip-en"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@mdi/js": "^5.9.55",
    "@reorx/webpack-ext-reloader": "github:ttop32/webpack-ext-reloader",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^4.3.0",
    "deepmerge": "^4.2.2",
    "del-cli": "^5.0.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.1",
    "npm-build-zip": "^1.0.3",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "size-plugin": "^2.0.2",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.2.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@mswjs/interceptors": "^0.24.1",
    "async-wait-until": "^2.0.12",
    "bcp-47": "^2.0.0",
    "bootstrap": "^4.5.3",
    "crx-hotreload": "^1.0.6",
    "crypto-browserify": "^3.12.0",
    "delay": "^6.0.0",
    "he": "^1.2.0",
    "is-url": "^1.2.4",
    "jquery": "^3.5.1",
    "ky": "^1.0.1",
    "maska": "^2.1.10",
    "match-url-wildcard": "^0.0.5",
    "popper.js": "^1.16.1",
    "source-map-support": "^0.5.21",
    "stream-browserify": "^3.0.0",
    "tesseract.js": "^4.1.1",
    "throttle-debounce": "^5.0.0",
    "tippy.js": "^6.3.7",
    "typeface-roboto": "^1.1.13",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vuetify": "^3.3.16",
    "webextension-polyfill": "^0.10.0",
    "whatwg-fetch": "^3.6.18",
    "window-post-message-proxy": "^0.2.6"
  }
